            Tech Stack - Tools I Use Everyday
          </div>
          <div class="relative mb-20">
            <div class="w-20 absolute left-0 top-0 bottom-0 z-10 bg-gradient-to-r from-white to-transparent"></div>
            <div class="w-20 absolute right-0 top-0 bottom-0 z-10 bg-gradient-to-l from-white to-transparent"></div>
            <div class="swiper">
              <div class="swiper-wrapper">
                {{#each technology}}
                <div class="swiper-slide">
                  <div class="flex flex-col gap-3 items-center">
                    <div
                      class="w-16 h-16 shadow-sm rounded-xl p-3 hover:shadow-xl transform duration-700 flex items-center justify-center">
                      <img src="../assets/uploads/{{this.icon}}" class="object-contain" alt="" />
                    </div>
                    <span class="text-gray-600 text-sm">{{this.nametech}}</span>
                  </div>
                </div>
                {{/each}}
              </div>
            </div>
          </div>
          

          <h2 class="font-bold text-3xl mb-20">Work Experiences:</h2>
          <div class="p-6 flex flex-col gap-10 mb-20">
            {{#each experience}}
            <div class="flex gap-10 shadow-sm px-4 py-12">
              <div class="h-16 w-16 mt-3">
                <img src="../assets/uploads/{{this.logo}}" class="" alt="" />
                </div>
                <div class="w-full">
                  <div class="">
                    <div class="flex justify-between">
                      <div class="">
                      <h3 class="text-lg font-semibold">
                        {{this.namejob}}
                      </h3>
                      <p class="text-green-600">
                        {{this.company}}
                      </p>
                      </div>
                    <span class="text-gray-500">{{this.datestart}} - {{this.dateend}}</span>
                    </div>
                    </div>
                    <div class="mt-3">
                  <ul class="list-inside list-disc mb-4 space-y-2 text-gray-600 text-sm">
                    {{#each activity}}
                    <li>{{this}}</li>
                    {{/each}}
                    </div>
                <div class="flex gap-2 mt-2">
                  {{#each technology}}
                  <span class="rounded-full bg-gray-200 text-xs font-medium px-3 py-1 text-gray-600">{{this}}</span>
                  {{/each}}
                </div>
                </div>
                </div>
          {{/each}}
          </div>
          <h2 class="text-3xl font-bold mb-20">My Projects:</h2>
          <div class="grid grid-cols-2 gap-8 mb-20" id="cardProjects">
            {{#each project}}
            <div class="overflow-hidden rounded-xl shadow-sm">
              <div class="relative h-52 flex bg-gray-200 items-center justify-center bg-gray">
                <img src="../assets/uploads/{{this.pic}}" class="w-full h-full object-contain rounded-xl" alt="" />
              </div>
              <div class="p-8">
                <h3 class="font-bold text-xl mb-3">{{this.nameproject}}</h3>
                <p class="text-gray-600 text-sm mb-4">{{this.description}}</p>
                <div class="flex gap-2 mt-2 mb-6">
                  {{#each this.technology}}
                  <span class="rounded-full bg-gray-200 text-xs font-medium px-3 py-1 text-gray-600">{{this}}</span>
                  {{/each}}
                </div>
                <div class="flex gap-4">
